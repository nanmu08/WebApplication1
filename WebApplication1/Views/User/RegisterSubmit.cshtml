
@model DataAccess.Models.Users

@{
    ViewBag.Title = "RegisterSubmit";
}


<div class="form-group row">

    <div class=" col-form-label col-sm-8 offset-sm-2">
        <div class="card">
            <div class="card-header">
                Success! Your Register Information
            </div>
            <div class="card-body">
                <ul class="list-group list-group-flush">
                    
                    <li class="list-group-item">User Name: @Model.UserName</li>
                    <li class="list-group-item">Email Address: @Model.Email</li>
                    <li class="list-group-item">Password: @Model.Password</li>
                </ul>
            </div>
        </div>
    </div>

</div>